{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends":[
      "standard-with-typescript",
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "rules": {
      "prettier/prettier":"error"
    },
    "settings": {
      "import/resolver":{
        "typescript":{}
      }
    }
}
